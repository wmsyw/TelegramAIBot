{
  "name": "telegram-ai-bot",
  "version": "2.0.0",
  "description": "Multi-provider AI Telegram Bot with per-user settings, session modes, and Gemini Live support",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["telegram", "bot", "ai", "openai", "gemini", "claude", "live"],
  "license": "MIT",
  "dependencies": {
    "grammy": "^1.21.1",
    "axios": "^1.6.7",
    "better-sqlite3": "^11.0.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4",
    "dotenv": "^16.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/better-sqlite3": "^7.6.9",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
